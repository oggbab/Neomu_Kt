<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <!-- TitleFont -->
    <!-- normal -->
    <font
        android:font="@font/serif_r"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/serif_r"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/serif_b"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/serif_b"
        app:fontStyle="normal"
        app:fontWeight="700" />


    <!-- ContentFont -->
    <!-- normal -->
    <font
        android:font="@font/sans_r"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/sans_r"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/sans_b"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/sans_b"
        app:fontStyle="normal"
        app:fontWeight="700" />



</font-family>
